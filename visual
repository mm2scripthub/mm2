local success, err = pcall(function()

    -- Executor adı bul

    local executorName = identifyexecutor and identifyexecutor() or "Unknown"

    -- Sadece Delta için devreye girsin

    if executorName ~= "Delta" then

        return

    end

    -- Tam ekran warning screen

    local screenGui = Instance.new("ScreenGui")

    screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

    screenGui.Name = "WarningScreen"

    screenGui.ResetOnSpawn = false

    screenGui.IgnoreGuiInset = true

    local frame = Instance.new("Frame")

    frame.Parent = screenGui

    frame.Size = UDim2.new(1, 0, 1, 0)

    frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)

    frame.BackgroundTransparency = 0

    frame.BorderSizePixel = 0

    local label = Instance.new("TextLabel")

    label.Parent = frame

    label.Size = UDim2.new(0.9, 0, 0.9, 0) 

    label.Position = UDim2.new(0.05, 0, 0.05, 0) 

    label.Text = [[

⚠️ WARNING FOR DELTA USERS ⚠️

If you see this message, make sure to turn off Anti-Scam before executing the script!  

If you haven't turned it off, rejoin the game, disable Anti-Scam, and try again.  

Otherwise, the script will not work!

This screen will close after 20 seconds...

]]

    label.TextColor3 = Color3.fromRGB(255, 215, 0)

    label.BackgroundTransparency = 1

    label.TextSize = 24

    label.Font = Enum.Font.GothamBold

    label.TextWrapped = true

    label.TextStrokeTransparency = 0.2

    label.TextStrokeColor3 = Color3.fromRGB(255, 140, 0)

    label.TextXAlignment = Enum.TextXAlignment.Center

    label.TextYAlignment = Enum.TextYAlignment.Center

    -- 20 saniye sonra kapat

    task.delay(20, function()

        if screenGui then

            screenGui:Destroy()

        end

    end)

end)

if not success then

    warn("Error: " .. err)

end
